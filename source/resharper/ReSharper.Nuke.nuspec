<?xml version="1.0" encoding="utf-8"?>

<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <title>NUKE Support</title>
    <id>ReSharper.Nuke</id>
    <version>$Version$</version>
    <authors>Sebastian Karasek, Matthias Koch</authors>
    <owners>arodus</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <projectUrl>https://nuke.build</projectUrl>
    <licenseUrl>https://github.com/nuke-build/resharper/blob/master/LICENSE</licenseUrl>
    <iconUrl>https://raw.githubusercontent.com/nuke-build/nuke/master/icon.png</iconUrl>
    <description>
      Adds support for NUKE. A cross-platform build automation system for C# developers.
      
      &#8226; Highlights build targets with an icon in the editor gutter.
      &#8226; Context action for target execution (run/debug and with/without dependencies).
    </description>
    <releaseNotes>$ReleaseNotes$</releaseNotes>
    <dependencies>
      <dependency id="Wave" version="[$Wave$]" />
    </dependencies>
    <tags>nuke build</tags>
  </metadata>
  <files>
    <file src="bin\ReSharper.Nuke\$Configuration$\ReSharper.Nuke.dll" target="DotFiles" />
  </files>
</package>